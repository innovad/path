  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innovad/path.git"
  },
  "engines": {
    "node": "12.13.1",
    "npm": "6.12.1"
  },
  "scripts": {
    "build": "ng lint && ng test --watch=false --browsers Firefox && ng-packagr -p package.json",
